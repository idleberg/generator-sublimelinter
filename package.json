{
	"name": "generator-sublimelinter",
	"version": "0.2.2",
	"description": "Yeoman generator for SublimeLinter plugins",
	"author": "Jan T. Sott",
	"license": "MIT",
	"keywords": [
		"yeoman-generator",
		"sublimelinter",
		"sublimelinter-plugin",
		"sublime text"
	],
	"type": "module",
	"exports": "./generators/app",
	"files": [
		"generators",
		"LICENSE",
		"README.md"
	],
	"scripts": {
		"fix": "npx biome check --apply ./generators",
		"lint": "npx biome check ./generators",
		"prepack": "rimraf linter.py",
		"prepare": "husky",
		"publish": "np --no-yarn",
		"test": "uvu --ignore test/__helper.js"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/idleberg/generator-sublimelinter"
	},
	"engines": {
		"node": ">=18"
	},
	"dependencies": {
		"@sindresorhus/slugify": "^2.2.1",
		"pascal-case": "^4.0.0",
		"semver": "^7.6.0",
		"spdx-license-list": "^6.9.0",
		"terminal-link": "^3.0.0",
		"yeoman-generator": "^7.1.1"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.6.4",
		"@lukeed/uuid": "^2.0.1",
		"eslint-plugin-json": "^3.1.0",
		"husky": "^9.0.11",
		"mem-fs": "^4.1.0",
		"np": "^10.0.2",
		"rimraf": "^5.0.5",
		"tsm": "^2.3.0",
		"uvu": "^0.5.6",
		"yeoman-assert": "^3.1.1",
		"yeoman-environment": "^4.4.0",
		"yeoman-test": "^8.2.0"
	},
	"packageManager": "pnpm@9.0.5+sha256.61bd66913b52012107ec25a6ee4d6a161021ab99e04f6acee3aa50d0e34b4af9"
}

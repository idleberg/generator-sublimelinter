{
	"name": "generator-sublimelinter",
	"version": "0.3.1",
	"description": "Yeoman generator for SublimeLinter plugins",
	"author": "Jan T. Sott",
	"license": "MIT",
	"keywords": [
		"yeoman-generator",
		"sublimelinter",
		"sublimelinter-plugin",
		"sublime text"
	],
	"type": "module",
	"exports": "./generators/app/index.js",
	"files": [
		"generators",
		"LICENSE",
		"README.md"
	],
	"scripts": {
		"lint:e18e": "#e18e-cli analyze",
		"lint:ejs": "ejslint generators/**/*.ejs",
		"lint:code": "biome check",
		"lint": "concurrently --prefix '{name}' -c 'green,blue' 'npm:lint:*'",
		"prepare": "lefthook install",
		"publish:npm": "np --no-yarn",
		"test": "vitest run",
		"test:watch": "vitest"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/idleberg/generator-sublimelinter"
	},
	"engines": {
		"node": ">=20"
	},
	"dependencies": {
		"@sindresorhus/slugify": "^3.0.0",
		"change-case": "^5.4.4",
		"semver": "^7.7.3",
		"spdx-license-list": "^6.10.0",
		"terminal-link": "^5.0.0",
		"yeoman-generator": "^7.5.1"
	},
	"devDependencies": {
		"@commitlint/cli": "^20.3.0",
		"@commitlint/config-conventional": "^20.3.0",
		"@idleberg/configs": "^0.4.1",
		"@types/node": "^24.10.4",
		"@types/yeoman-assert": "^3.1.4",
		"@yeoman/adapter": "^4.0.1",
		"@yeoman/types": "^1.8.0",
		"concurrently": "^9.2.1",
		"ejs-lint": "^2.0.1",
		"mem-fs": "^4.1.2",
		"np": "^10.2.0",
		"vitest": "^4.0.16",
		"yeoman-assert": "^3.1.1",
		"yeoman-environment": "^5.1.1",
		"yeoman-test": "^11.2.0"
	},
	"packageManager": "pnpm@10.27.0+sha512.72d699da16b1179c14ba9e64dc71c9a40988cbdc65c264cb0e489db7de917f20dcf4d64d8723625f2969ba52d4b7e2a1170682d9ac2a5dcaeaab732b7e16f04a"
}

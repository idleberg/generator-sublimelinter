    "@sindresorhus/slugify": "^0.4.0",
    "git-user-name": "^2.0.0",
    "mkdirp": "^0.5.1",
    "pascal-case": "^2.0.1",
    "semver": "^5.6.0",
    "spdx-license-list": "^4.1.0",
    "terminal-link": "^1.1.0",
    "update-notifier": "^2.5.0",
    "yeoman-generator": "^3.1.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "ejs-lint": "^0.3.0",
    "eslint": "^5.8.0",

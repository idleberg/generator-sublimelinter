{
	"name": "generator-sublimelinter",
	"version": "0.2.2",
	"description": "Yeoman generator for SublimeLinter plugins",
	"author": "Jan T. Sott",
	"license": "MIT",
	"keywords": [
		"yeoman-generator",
		"sublimelinter",
		"sublimelinter-plugin",
		"sublime text"
	],
	"type": "module",
	"exports": "./generators/app",
	"files": [
		"generators",
		"LICENSE",
		"README.md"
	],
	"scripts": {
		"lint:e18e": "#e18e-cli analyze",
		"lint:ejs": "ejslint generators/**/*.ejs",
		"lint:code": "biome check",
		"lint": "concurrently --prefix '{name}' -c 'green,blue' 'npm:lint:*'",
		"prepare": "lefthook install",
		"publish": "np --no-yarn",
		"test": "uvu --ignore test/__helper.js"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/idleberg/generator-sublimelinter"
	},
	"engines": {
		"node": ">=18"
	},
	"dependencies": {
		"@sindresorhus/slugify": "^2.2.1",
		"pascal-case": "^4.0.0",
		"semver": "^7.6.0",
		"spdx-license-list": "^6.9.0",
		"terminal-link": "^3.0.0",
		"yeoman-generator": "^7.1.1"
	},
	"devDependencies": {
		"@commitlint/cli": "^20.3.0",
		"@commitlint/config-conventional": "^20.3.0",
		"@idleberg/configs": "^0.4.1",
		"@lukeed/uuid": "^2.0.1",
		"concurrently": "^9.2.1",
		"ejs-lint": "^2.0.1",
		"eslint-plugin-json": "^3.1.0",
		"mem-fs": "^4.1.0",
		"np": "^10.0.2",
		"rimraf": "^5.0.5",
		"tsm": "^2.3.0",
		"uvu": "^0.5.6",
		"yeoman-assert": "^3.1.1",
		"yeoman-environment": "^4.4.0",
		"yeoman-test": "^8.2.0"
	},
	"packageManager": "pnpm@10.27.0+sha512.72d699da16b1179c14ba9e64dc71c9a40988cbdc65c264cb0e489db7de917f20dcf4d64d8723625f2969ba52d4b7e2a1170682d9ac2a5dcaeaab732b7e16f04a"
}
